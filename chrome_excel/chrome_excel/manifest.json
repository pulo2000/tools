{
    "manifest_version": 3,
    "name": "excel比对工具",
    "version": "1.1.0",
    "description": "excel比对工具",
    "icons": {
        "16": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png"
    },
    "action": {
        "default_icon": {
            "16": "img/icon.png",
            "24": "img/icon.png",
            "32": "img/icon.png"
        },
        "default_title": "excel比对工具"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  }
} 
   
   
   